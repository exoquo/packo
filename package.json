{
  "name": "laravue",
  "version": "0.11.0",
  "description": "A beautiful dashboard for Laravel built by VueJS",
  "author": "Tuan Duong <bacduong@gmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "npm run development",
    "development": "cross-env NODE_ENV=development BABEL_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "watch": "npm run development -- --watch",
    "watch-poll": "npm run watch -- --watch-poll",
    "hot": "cross-env NODE_ENV=development BABEL_ENV=development node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --hot --config=node_modules/laravel-mix/setup/webpack.config.js",
    "prod": "npm run production",
    "production": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --no-progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "report": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --no-progress --hide-modules --report --config=node_modules/laravel-mix/setup/webpack.config.js",
    "lint": "eslint --ext .js,.vue resources/js",
    "test": "mocha-webpack --webpack-config=webpack.config.js --require tests/Javascript/setup.js tests/Javascript/**/*.spec.js"
  },
  "lint-staged": {
    "resources/**/*.{js,vue}": [
      "eslint --fix",
      "git add"
    ]
  },
  "devDependencies": {
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@vue/test-utils": "^1.0.3",
    "babel-eslint": "^10.1.0",
    "babel-plugin-dynamic-import-node": "^2.3.3",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-vue-jsx": "^3.7.0",
    "browser-sync": "^2.26.7",
    "browser-sync-webpack-plugin": "^2.2.2",
    "compression-webpack-plugin": "^3.1.0",
    "cross-env": "^7.0.2",
    "eslint": "^6.8.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-vue": "^6.2.2",
    "expect": "^25.5.0",
    "husky": "^4.2.5",
    "jsdom": "^16.2.2",
    "jsdom-global": "^3.0.2",
    "laravel-mix": "^5.0.4",
    "laravel-mix-eslint": "^0.1.3",
    "lint-staged": "^10.2.11",
    "mocha": "^7.2.0",
    "mocha-webpack": "^v2.0.0-beta.0",
    "preload-webpack-plugin": "^3.0.0-beta.4",
    "resolve-url-loader": "^3.1.1",
    "sass-loader": "^8.0.2",
    "script-loader": "^0.7.2",
    "svg-sprite-loader": "^4.3.0",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^4.43.0",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.3.12"
  },
  "dependencies": {
    "@babel/polyfill": "^7.10.4",
    "axios": "^0.19.2",
    "babel-loader": "^8.1.0",
    "camelcase": "^6.0.0",
    "clipboard": "^2.0.6",
    "core-js": "^3.6.5",
    "css-loader": "^3.6.0",
    "driver.js": "^0.9.7",
    "dropzone": "^5.7.1",
    "echarts": "^4.8.0",
    "element-ui": "^2.13.2",
    "file-loader": "^6.0.0",
    "file-saver": "^2.0.2",
    "fuse.js": "^5.2.3",
    "js-cookie": "^2.2.1",
    "jsonlint": "^1.6.3",
    "jszip": "^3.5.0",
    "node-sass": "^4.14.1",
    "normalize.css": "^8.0.1",
    "nprogress": "^0.2.0",
    "path-to-regexp": "^6.1.0",
    "quagga": "^0.12.1",
    "screenfull": "^5.0.2",
    "sortablejs": "^1.10.2",
    "tui-editor": "^1.4.6",
    "vue": "^2.6.11",
    "vue-count-to": "^1.0.13",
    "vue-i18n": "^8.18.2",
    "vue-loader": "^15.9.3",
    "vue-router": "^3.3.4",
    "vue-splitpane": "^1.0.4",
    "vuedraggable": "^2.23.0",
    "vuex": "^3.5.1",
    "xlsx": "^0.16.3"
  }
}
